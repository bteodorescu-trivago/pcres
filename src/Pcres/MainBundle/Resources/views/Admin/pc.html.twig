<form action="{{ path('_main') }}" method="post" {{ form_enctype(formPc) }} class="form-horizontal">
    {{ form_errors(formPc) }}

    {% for child in formPc.children %}

        <div class="control-group">
            <label class="control-label" for="input{{ child.vars.label }}">{{ child.vars.label }}</label>
            <div class="controls">
                {{ form_widget(child) }}
            </div>
        </div>
    {% endfor %}

    {{ form_rest(formPc) }}
</form>